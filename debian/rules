#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

export DEBUILD_DPKG_BUILDPACKAGE_OPTS="-I.svn -I.git"

.PHONY: override_dh_strip override_dh_makeshlibs

override_dh_strip:
	dh_strip -plibcoil --dbg-package=libcoil-dbg
	dh_strip -plibopenrtm-aist-c++ --dbg-package=libopenrtm-aist-c++-dbg

override_dh_makeshlibs:
	dh_makeshlibs -plibcoil
	dh_makeshlibs -plibopenrtm-aist-c++

%:
	dh  $@
